{
  "version": "0.2",
  "language": "en",
  "noConfigSearch": true,
  "useGitignore": true,
  "globRoot": "../",
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/.pnpm-lock.json",
    ".vscode",
    "megalinter",
