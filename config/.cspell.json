{
  "version": "0.2",
  "language": "en",
  "noConfigSearch": true,
  "useGitignore": true,
  "globRoot": "../",
  "ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/.pnpm-lock.json",
    ".vscode",
    "megalinter",
    "package.json",
    "package-lock.json",
    "report",
    "**/public/**",
    "**/.github/**",
    ".gitignore",
    ".mega-linter.yml",
    "config/.jscpd.json",
    "infra/*.tf",
    "docs/layouts/*.html",
    "docs/config/*.config.js",
    "docs/assets/**",
    "docs/config/_default/**/*.toml",
    "infra/config/cloud-init-agent.yml",
    "infra/config/cloud-init-server.yml",
    "**/k3s/**"
  ],
  "words": [
    "megalinter",
    "oxsecurity",
    "homelab",
    "iamkhattar",
    "unmark",
    "peaceiris",
    "tfstate",
    "tfvars",
    "terraformrc",
    "Shivam",
    "Khattar",
    "hetznercloud",
    "svgs",
    "npmignore",
    "venv",
    "tfrc",
    "instantpage"
  ]
}
